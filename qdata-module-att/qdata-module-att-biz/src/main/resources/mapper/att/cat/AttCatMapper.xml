<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tech.qiantong.qdata.module.att.dal.mapper.cat.AttCatMapper">

    <select id="getCatIdByTableNameAndCatCode" resultType="java.lang.Long">
        SELECT
            ID
        FROM
            ${tableName}
        WHERE
            CODE = #{catCode} AND DEL_FLAG = 0
    </select>
</mapper>
